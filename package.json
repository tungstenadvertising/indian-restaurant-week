    "imagemin-svgo": "^11.0.1",
    "imagemin-webp": "^8.0.0",
    "sharp": "^0.33.0",
    "tailwindcss": "^4.1.18",
    "terser": "^5.0.0",
    "vite": "^7.3.0",
    "vite-plugin-imagemin": "^0.6.1",
